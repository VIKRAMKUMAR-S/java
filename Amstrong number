import java.util.*;
class Main{
    public static void main(String args[]){
    Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    String s=Integer.toString(n);
    int q=s.length();
    double c=0;
    int a;
    while(n>0){
        a=n%10;
        double b=(Math.pow(a,q));
        c+=b;
        n=n/10;
    }
    int r=(int)c;
    System.out.print(r);
    }
    }
